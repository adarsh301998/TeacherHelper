<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXToggleButton?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIcon?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<StackPane fx:id="stack_pane" stylesheets="@../../../layouts/CSS/maincss.css" xmlns="http://javafx.com/javafx/8.0.141"
           xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="sample.Controllers.ReportControllers.DisplayStudentResponseController">
    <AnchorPane prefHeight="400.0" prefWidth="741.0">
        <children>
            <AnchorPane prefHeight="49.0" prefWidth="746.0" styleClass="tittlePaneBackground"
                        AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                <children>
                    <FontAwesomeIcon fx:id="close_btn" glyphName="CLOSE" layoutX="717.0" layoutY="32.0"
                                     onMouseClicked="#closeEvent" size="3em" styleClass="close-icon"/>
                    <JFXButton fx:id="back_btn" layoutX="7.0" layoutY="3.0" onAction="#backFrameEvent" prefHeight="29.0"
                               prefWidth="62.0" text=" ">
                        <graphic>
                            <ImageView fitHeight="32.0" fitWidth="64.0" pickOnBounds="true" preserveRatio="true">
                                <image>
                                    <Image url="@../../../../icons/back.png"/>
                                </image>
                            </ImageView>
                        </graphic>
                    </JFXButton>
                    <Label fx:id="studentResponseLabel" layoutX="69.0" layoutY="9.0"
                           onMouseClicked="#displayBasicTestInfo" styleClass="titlePaneHeading"
                           text="Student Response"/>
                </children>
            </AnchorPane>
            <JFXToggleButton fx:id="binaryToggle" layoutX="230.0" layoutY="49.0" onAction="#toggleActionEvent"
                             text="Binary Information">
                <font>
                    <Font name="Segoe UI" size="24.0"/>
                </font>
            </JFXToggleButton>
            <TableView fx:id="tableView" layoutX="44.0" layoutY="126.0" prefHeight="267.0" prefWidth="680.0"/>
        </children>
    </AnchorPane>
</StackPane>
